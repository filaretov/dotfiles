include gruvbox_dark.conf

disable_ligatures always
cursor_shape beam
cursor_blink_interval 0
input_delay 1

macos_option_as_alt yes


allow_remote_control true

shell /Users/hgf/.cargo/bin/nu

map F5 launch --location=hsplit
# Unmap
map super+,
map super+r

# BEGIN_KITTY_FONTS
font_family      family="Iosevka Term"
bold_font        auto
italic_font      auto
bold_italic_font auto
# END_KITTY_FONTS
font_size        14
# text_composition_strategy 0.5 0
text_composition_strategy legacy
